train_net: "../caffe/examples/vgps/train_val_ioc_quadratic_slow.prototxt"
solver_type: ADAM
random_seed: 11
average_loss: 50
base_lr: 0.001 # was 0.008
lr_policy: "step"
gamma: 0.1
stepsize: 100000
display: 100
max_iter: 8000  # 8000 iterations for cold starting
momentum: 0.9
weight_decay: 0 # 0.000001 # 0.005
snapshot: 100000
snapshot_prefix: "matnet7"
solver_mode: GPU
