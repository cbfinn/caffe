train_net: "../caffe/examples/vgps/train_val_ioc_adversary.prototxt"
solver_type: SGD
# average_loss: 50
#test_interval: 2000
base_lr: 0.008 # was 0.008
lr_policy: "step"
gamma: 0.1
stepsize: 100000
