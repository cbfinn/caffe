  type: HDF5_OUTPUT
  hdf5_output_param {
    file_name: "test_output/conv2_trapezoid_vis2.h5"
  }
  bottom: "conv2"
  bottom: "rgb_frames"
}
layers {
  name: "output_heatmap1"
  type: HDF5_OUTPUT
  hdf5_output_param {
    file_name: "test_output/conv3_trapezoid_vis2.h5"
  }
  bottom: "conv3"
  bottom: "rgb_frames"
}
layers {
  name: "output_heatmap2"
  type: HDF5_OUTPUT
  hdf5_output_param {
    file_name: "test_output/conv1_trapezoid_vis2.h5"
  }
  bottom: "conv1"
  bottom: "rgb_frames"
}
