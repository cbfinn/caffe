train_net: "../caffe/examples/vgps/deploy_ioc_vis.prototxt"
solver_type: SGD
# average_loss: 50
#test_interval: 2000
base_lr: 0.01 # was .008 before 0.5 change
lr_policy: "step"
gamma: 0.1
stepsize: 8000
display: 100
max_iter: 8000  # 8000 iterations for cold starting
momentum: 0.9
#weight_decay: 0.005
weight_decay: 0
snapshot: 8000
snapshot_prefix: "ioc_vis"
solver_mode: GPU
