net: "examples/vgps/train_val_rgb.prototxt"
solver_type: SGD
test_iter: 10
test_interval: 500
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 8000
display: 100
max_iter: 12000
momentum: 0.9
weight_decay: 0.005
snapshot: 5000
snapshot_prefix: "examples/vgps/snapshot/train_images_rgb"
solver_mode: GPU
